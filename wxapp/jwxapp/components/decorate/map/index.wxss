.map-container {
  font-size: 28rpx;
  width: 715rpx;
  margin: 0 auto;
  margin-bottom: 20rpx;
}

.map_title {
  padding: 20rpx 0;
  background: #fff;
  display: flex;
  align-items: center;
}

.map_title image {
  width: 20rpx;
  height: 30rpx;
  margin-right: 20rpx;
  margin-left: 40rpx;
}

.map_content {
  width: 100%;
  height: 300rpx;
  overflow: hidden;
  background: #fff;
}

.map_content image {
  width: 100%;
  height: 300rpx;
  border-radius: 5rpx;
}
