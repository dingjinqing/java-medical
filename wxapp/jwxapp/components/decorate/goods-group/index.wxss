@import "../product/goodscommon/goodslabel.wxss";
@import "/css/iconfont.wxss";

.goods-group-container {
  margin-bottom: 10rpx;
  overflow: hidden;
}

.goods_group_nav {
  white-space: nowrap;
  height: 80rpx;
  background: #fff;
  line-height: 80rpx;
  display: flex;
  /* overflow: hidden; */
  
}

.goods_group_scroll {
  background: #fff;
}

.group_container {
  background: #fff;
  overflow: hidden;
}

.click_see_more {
  margin-bottom: 30rpx;
  overflow: hidden;
}

.goods_left_module .click_see_more {
  margin-bottom: 20rpx;
  overflow: hidden;
  margin-top: 20rpx;
}

.click_see {
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0rpx auto;
  color: #999;
  font-size: 24rpx;
  height: 50rpx;
  width: 180rpx;
  border: 1rpx solid #ccc;
  border-radius: 25rpx;
  text-align: center;
}

.click_see image {
  width: 20rpx;
  height: 12rpx;
  margin-left: 6rpx;
}

.slide_click {
  display: inline-block;
  margin-bottom: 20rpx;
  position: relative;
  height: 410rpx;
  width: 280rpx;
  overflow: hidden;
  vertical-align: top;
}

.slide_click image {
  width: 140px;
  height: 200px;
}

.color_red {
  color: #f66;
}

.group_single_flex {
  flex: 1;
}

.group_double_flex {
  flex: 0 0 22%;
}

.group_nav_title {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.group_nav_single {
  position: relative;
  overflow: hidden;
  display: inline-block;
}

.group_nav_single_content {
  text-align: center;
}

.nav_bottom_select {
  position: absolute;
  bottom: -1rpx;
  border: 2rpx solid #f66;
  width: 100%;
  left: 50%;
  transform: translateX(-50%);
}

.circle_radius {
  border-radius: 16rpx;
}

.have_border {
  border: 1rpx solid #ddd;
}

.have_shadow {
  box-shadow: 0 0 15rpx 6rpx #e5e5e5;
}

.content_scroll_view {
  margin-bottom: 10rpx;
  height: 80rpx;
}

.nav_fixed {
  position: fixed;
  z-index: 9;
  top: -2rpx;
}

.group_delete {
  position: absolute;
  bottom: 0rpx;
  background: rgba(0, 0, 0, 0.5);
  color: #fff;
  font-size: 24rpx;
  padding: 10rpx 0;
  width: 100%;
  text-align: center;
}

/* 分组 */

.sale_price {
  display: inline-block;
  max-width: 65%;
}

.market_price {
  font-weight: normal;
  margin-left: 10rpx;
  margin-top: 0;
  color: #999;
  text-decoration: line-through;
  float: right;
}

.line-through-none {
  text-decoration: none !important;
}

.goods_group_module {
  padding: 10rpx 10rpx;
  overflow: hidden;
}

.module_goods_price {
  color: #f66;
}

.module_img {
  overflow: hidden;
  position: relative;
}

.module_img .save_money {
  bottom: 0rpx !important;
}

.group_double_module .save_money {
  bottom: 6rpx !important;
  width: 280rpx;
  color: #fff;
  font-size: 28rpx;
  text-align: center;
  border-radius: 0 80rpx 80rpx 0;
  position: absolute;
  left: 0;
  z-index: 4;
  height: 55rpx;
  line-height: 55rpx;
}

.group_list_module .save_money {
  bottom: 0rpx !important;
  width: 240rpx;
  color: #fff;
  font-size: 26rpx;
  text-align: center;
  border-radius: 0 80rpx 80rpx 0;
  position: absolute;
  left: 0;
  z-index: 4;
  height: 50rpx;
  line-height: 50rpx;
}

.goods_left_module .save_money {
  bottom: 0rpx !important;
  width: 200rpx;
  color: #fff;
  font-size: 22rpx;
  text-align: center;
  border-radius: 0 80rpx 80rpx 0;
  position: absolute;
  left: 0;
  z-index: 4;
  height: 45rpx;
  line-height: 45rpx;
}

.module_text {
  overflow: hidden;
}

.group_big_module, .group_double_module {
  position: relative;
  margin-bottom: 6rpx;
  overflow: visible !important;
}

/* 大图 */

.group_big_module .module_img {
  height: 300rpx;
}

.group_big_module .module_img image {
  width: 100%;
}

.group_big_module .group_container {
  margin-bottom: 20rpx;
}

.group_big_module .module_text {
  margin: 10rpx 0;
  height: auto;
}

.group_big_module .module_text .module_goods_price {
  font-size: 28rpx;
  font-weight: bold;
  padding: 0 10rpx;
}

.group_big_module .module_text .module_goods_price .sale_price {
  font-size: 30rpx;
  margin-top: -3rpx;
}

.group_big_module .module_text .module_goods_name {
  height: 90rpx;
  margin-bottom: 1px;
}

/* 双列图 */

.group_double_module .group_container {
  float: left;
  margin-bottom: 20rpx;
  width: 350rpx;
  margin-left: 10rpx;
}

.group_double_module .module_img image {
  height: 350rpx;
  width: 350rpx;
  margin: 0 auto;
  overflow: hidden;
  position: relative;
}

.group_double_module .module_text {
  margin: 5rpx 0;
  height: 150rpx;
}

.group_double_module .module_text .module_goods_price {
  font-size: 28rpx;
  font-weight: bold;
  padding: 0rpx 10rpx;
}

.group_double_module .module_text .module_goods_price .sale_price {
  font-size: 30rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-top: 1rpx;
}

.group_double_module .module_text .module_goods_price .market_price {
  margin-top: 6rpx;
}

.group_double_module .module_text .module_goods_name {
  height: 80rpx;
  padding-left: 8rpx;
  margin-bottom: 6rpx;
}

.group_double_module .right_buy_btn {
  width: 110rpx;
  height: 45rpx;
  line-height: 45rpx;
  font-size: 22rpx;
}

.group_double_module .cart_buy_btn {
  width: 90rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 22rpx;
}

/* 三列图 */

.group_trible_module {
  position: relative;
  overflow: hidden;
  margin-left: 5rpx;
}

.group_trible_module .group_container {
  float: left;
  margin-bottom: 20rpx;
  width: 230rpx;
  margin-left: 8rpx;
}

.group_trible_module .module_img {
  height: 230rpx;
}

.group_trible_module .module_img image {
  height: 230rpx;
  width: 230rpx;
  margin: 0 auto;
  overflow: hidden;
  position: relative;
}

.group_trible_module .module_text {
  margin: 5rpx 0;
  /* height: 120rpx; */
}

.group_trible_module .module_text .module_goods_price {
  font-size: 28rpx;
  font-weight: bold;
  padding: 8rpx 10rpx;
}

.group_trible_module .module_text .module_goods_price .sale_price {
  width: 128rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.group_trible_module .module_text .module_goods_name {
  height: 80rpx;
  padding-left: 8rpx;
}

/* 横向滑动 */

.group_slide_module {
  white-space: nowrap;
  height: 410rpx;
  margin-bottom: 20rpx;
}

.group_slide_module .group_container {
  margin-bottom: 20rpx;
  width: 280rpx;
  height: 410rpx;
  margin-right: 15rpx;
  display: inline-block;
  overflow: hidden;
  vertical-align: top;
  position: relative;
}

.group_slide_module .module_img {
  height: 280rpx;
}

.group_slide_module .module_img image {
  height: 280rpx;
  width: 280rpx;
  margin: 0 auto;
  overflow: hidden;
  position: relative;
}

.group_slide_module .module_text {
  margin: 5rpx 0;
  /* height: 120rpx; */
}

.group_slide_module .module_text .module_goods_price {
  font-size: 28rpx;
  font-weight: bold;
  padding: 8rpx 10rpx;
}

.group_slide_module .module_text .module_goods_price .sale_price {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.group_slide_module .module_text .module_goods_name {
  height: 80rpx;
  padding-left: 8rpx;
}

/* 列表 */

.group_list_module {
  width: 700rpx;
  margin: 0 auto;
  position: relative;
  overflow: hidden;
  /* background: #fff; */
}

.group_list_module .group_container {
  margin-bottom: 20rpx;
  height: 280rpx;
  overflow: hidden;
  padding: 20rpx;
}

.group_list_module .module_img {
  float: left;
}

.group_list_module .module_img image {
  height: 280rpx;
  width: 280rpx;
  margin: 0 auto;
  overflow: hidden;
  position: relative;
  float: left;
}

.group_list_module .module_text {
  padding-left: 8rpx;
  overflow: hidden;
}

.group_list_module .module_text .module_goods_name {
  height: 190rpx;
  font-size: 26rpx;
  padding-left: 8rpx;
  position: relative;
}

.top_title {
  height: 70rpx;
}

.group_list_module .module_text .module_goods_price {
  font-weight: bold;
  padding: 8rpx 10rpx;
  height: 74rpx;
  display: flex;
  flex-direction: column;
}
.group_list_module .module_text .module_goods_price .market_price{
  margin-left: 4rpx!important;
}
.group_list_module .module_text .module_goods_price .sale_price {
  font-size: 30rpx;
  margin-top: -3rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.group_list_module .module_text .module_goods_price .market_price {
  float: none;
}

/* 左侧分组 */

.left_nav {
  height: 100%;
  width: 23%;
  float: left;
  background: #f5f5f5;
}

.left_icon {
  height: 10%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.left_title {
  width: 65%;
  font-size: 26rpx;
  color: #666;
}

.left_select {
  background: white;
  border-left: 2rpx solid #f66;
}

.goods_left_module {
  float: left;
  width: 77%;
  background: white;
  height: 100%;
  overflow-y: auto;
}

.left_container {
  border-top: 1rpx solid #eee;
}

/* 列表 */

.left_module_title {
  height: 10%;
  display: flex;
  align-items: center;
  padding-left: 10rpx;
  color: #333;
  background: #fff;
}

.goods_left_module .group_container {
  height: 260rpx;
  overflow: hidden;
  padding: 10rpx;
  margin: 10rpx;
}

.goods_left_module .module_img {
  float: left;
}

.goods_left_module .module_img image {
  height: 240rpx;
  width: 240rpx;
  margin: 0 auto;
  overflow: hidden;
  position: relative;
  float: left;
}

.goods_left_module .module_text {
  padding-left: 8rpx;
  overflow: hidden;
}

.goods_left_module .module_text .module_goods_name {
  height: 190rpx;
  font-size: 26rpx;
  padding-left: 8rpx;
  position: relative;
}

.goods_left_module .module_text .module_goods_price {
  font-weight: bold;
  padding: 8rpx 10rpx;
  height: 50rpx;
}

.goods_left_module .module_text .module_goods_price .sale_price {
  font-size: 30rpx;
  margin-top: -3rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

