.reduce_price {
  background-color: #f9dbdb;
  color: #f66;
  position: absolute;
  width: 100%;
  height: 45rpx;
  line-height: 45rpx;
  text-align: center;
  bottom: 0;
  font-size: 22rpx;
  z-index: 2;
}

.cart_btn {
  float: right;
}

.right_buy_btn {
  width: 120rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  color: white;
  font-size: 24rpx;
  border-radius: 30rpx;
}

.cart_buy_btn {
  width: 100rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  color: rebeccapurple;
  font-size: 24rpx;
  border-radius: 30rpx;
  background: white;
}

/* 限时降价 会员专享标签 */

.group_trible_module .grade, .group_slide_module .grade,
.goods_left_module .grade {
  font-size: 22rpx;
  bottom: 0rpx;
}

.group_double_module .grade {
  bottom: 6rpx;
}

.group_double_module .pin_flag {
  margin-top: 4rpx;
  border: 1px solid #408080;
}

.group_big_module .grade, .group_list_module .grade {
  bottom: 0rpx;
}

.group_list_module .title_label, .goods_left_module .title_label {
  position: absolute;
  bottom: 0rpx;
  left: 10rpx;
}

.module_goods_name {
  padding-left: 8rpx;
}

.exclusive {
  position: absolute;
  width: 100%;
  height: 45rpx;
  line-height: 45rpx;
  text-align: center;
  color: #56381b;
  background: linear-gradient(left, #eed9ad, #e7c38a);
  background: -webkit-linear-gradient(left, #eed9ad, #e7c38a);
  background: -moz-linear-gradient(left, #eed9ad, #e7c38a);
  bottom: 0rpx;
  font-size: 24rpx;
  z-index: 2;
}

.see_more{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  background: rgba(0, 0, 0, 0.5);
  z-index: 3;
}
.see_more>view{
  margin-bottom: 10rpx;
  color: white;
  font-size: 30rpx;
}
.see_line{
  width: 120rpx;
  height: 2rpx;
  background: #fff;
  vertical-align: top;
}
.member_icon{
  padding: 0rpx 10rpx;
  color: #fff;
  background: linear-gradient(to left,#e0c787,#d9ae69);
  border-radius: 6rpx;
  margin-right: 10rpx;
  font-size: 24rpx;
  line-height: 32rpx;
  display: inline-block;
  height: 34rpx;
}
.title_box{
  display: flex;
}
.title_label{
  display: flex;
  align-items: center;
  color:#fff;
  /* border:none */
}
.pin_flagThree {
  margin-left: 0!important;
  border: 1px solid #408080;
}