.system_info{
  background: rgba(51,51,51,0.16);
  border-radius: 10rpx;
  color: #fff;
  padding: 20rpx;
  margin: 15rpx 30rpx;
}
.liner-left{
  width: 120rpx;
  height:6rpx;
  background: linear-gradient(to right, #f5f5f5 , orange);
  border-radius:4rpx;
}
.liner-right{
  width: 120rpx;
  height:6rpx;
  background: linear-gradient(to left, #f5f5f5 , orange);
  border-radius:4rpx;
}
.medical-begin{
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 15rpx 0;
}
.begin{
  margin: 0 40rpx;
  color: orange;
  font-size: 32rpx;
  font-weight: bold;
}