.pro_title {
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  position: relative;
  font-size: 26rpx;
  color: #353535;
}

.coupon_list_item {
  margin-bottom: 20rpx;
  text-align: center;
  font-size: 18px;
  width: 690rpx;
  padding: 0 30rpx;
  margin-top: 20rpx;
  box-sizing: border-box;
}

.coupon_detail {
  font-size: 12px;
  position: relative;
  width: 690rpx;
}
.coupon_left {
  display: inline-block;
  position: relative;
  width: 210rpx;
  height: 200rpx;
  background: #f66;
  border-radius: 10rpx;
  color: #fff;
  font-size: 26rpx;
  padding: 40rpx 0px;
  vertical-align: top;
  border-right: none;
  box-sizing: border-box;
}
.coupon_left_text .text .text {
  margin-top: 30rpx;
  font-size: 36rpx;
}
.coupon_right {
  display: inline-block;
  width: 450rpx;
  text-align: left;
  position: relative;
  height: 200rpx;
  border: 1px solid #eee;
  border-left: none;
  font-size: 24rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
  padding-top: 16rpx;
}
.coupon_right .coupon_condition {
  margin-top: 5%;
}
.coupon_time {
  border-bottom: 1px dashed #eee;
  min-height: 72rpx;
  margin-top: 1%;
}
.get_new_coupon {
  display: inline-block;
  width: 33%;
  height: 20px;
  line-height: 20px;
  text-align: center;
  border-radius: 20px;
  position: relative;
  left: 63%;
  margin-top: 3%;
}
.get_new_coupon .button {
  width: 100%;
  height: 20px;
  line-height: 20px;
  text-align: center;
  border-radius: 20px;
  background: #fededf;
  color: #f66;
  font-size: 26rpx;
  padding: 0;
  border: none;
}
.got_coupon {
  color: #999;
  background: #fff;
}
@media all and (max-width: 360px) {
  .get_new_coupon {
    margin-top: 11rpx;
  }
}
@media all and (min-width: 400px) {
  .coupon_left_text .text {
    font-size: 39rpx;
  }
}
.coupon_back {
  height: 238rpx;
  width: 5%;
  position: absolute;
  left: 29%;
}

.coupon_left .vip_exclusive {
  position: absolute;
  left: 9rpx;
  top: 6rpx;
  line-height: 30rpx;
  width: 68rpx;
  text-align: center;
  color: #fff;
  background-color: rgba(0, 0, 0, 0.1);
  font-size: 20rpx;
  border-radius: 30rpx;
  margin-top: 0;
}
.end_time_r {
  position: relative;
  left: 102rpx;
  display: block;
}

.coupon_xian {
  height: 200rpx;
  width: 4rpx;
  display: inline-block;
  position: absolute;
  right: 0;
  top: 0;
  z-index: 9;
}
.coupon_xian .image {
  height: 200rpx;
  width: 4rpx;
}
