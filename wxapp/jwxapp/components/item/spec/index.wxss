@import '/css/item.wxss';
.num_minus,.num_plus{
  line-height: 50rpx;
  text-align: center;
  width: 50rpx;
  box-sizing: border-box;
}
.num_minus{
  border-right: 2rpx solid #eee;
}
.num_plus{
  border-left: 2rpx solid #eee;
}
.num_cfg{
  width: 160rpx;
  display: flex;
  border: 2rpx solid #eee;
  box-sizing: border-box;
}
.num_cfg input{
  flex: 1;
  text-align: center;
}