<wxs module="filter">
  includes = function (arr, target) {
    if (arr.length === 0) return false
    return arr.indexOf(target) === -1 ? false : true
  }
  module.exports = {
    includes:includes
  }
</wxs>
<view class="not-involved" wx:if="{{activity && filter.includes([1,3,4,5,8,10],activity.activityType)}}">
  <view style="color:{{main_setting.comColor}}">分销</view>
  <view>活动商品不参与分销</view>
</view>