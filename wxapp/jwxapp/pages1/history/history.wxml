<view class="body">
  <include src="/pages/common/header.wxml"/>
  <view class="main-container">
    <view class="filter" style="top:{{margin_top_nav + 8}}px">
      <view class="input_box">
        <view>
        <icon size="13" type="search" role="img"></icon>
        </view>
        <input placeholder="请输入搜索关键字"></input>
      </view>
    </view>
    <view class="content" style="margin-top:{{margin_top_nav * 2 + 116}}rpx">
      <block wx:if="{{dataList[0].length}}" wx:for="{{dataList}}" wx:for-item="pageItem" wx:key="pageIndex" wx:for-index="pageIndex">
        <block wx:for="{{pageItem}}" wx:key="cardNo" wx:for-index="itemIndex">
          <text class="date">{{item.date}}</text>
          <view class="goods_content">
          <block wx:for="{{item.goodsList}}" wx:for-item="goodsItem" wx:for-index="goodsIndex" wx:key="goodsIndex">
            <v-double-col-goods goods-data="{{goodsItem}}"/>
          </block>
          </view>
        </block>
      </block>
    </view>
  </view>
  <include src="/pages/common/footer.wxml"/>
</view>