var includes = function (arr, target) {
  if (arr.length === 0) return false
  return arr.indexOf(target) === -1 ? false : true
}
var substring = function (str,start,end) {
  if(!str) return ''
  return str.substring(start,end)
}
var getSex = function (type){
  var sexData = {
    '0':'男',
    '1':'女',
    '2':'未知'
  }
  return sexData[''+type]
}
module.exports = {
  includes: includes,
  substring: substring,
  getSex:getSex
}