.flex_outside_box {
  margin-top: 20rpx;
  background: #fff;
}
.flex_box {
  padding: 30rpx 30rpx;
  color: #353535;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.item_left {
  color: #888;
  margin-right: 10rpx;
  width: auto;
  min-width: 80rpx;
}
.item_middle {
  flex: 1;
  min-width: 0;
}
.item_right {
  width: auto;
}
.item_line {
  height: 2rpx;
  background-color: #eee;
  width: 700rpx;
  margin: 0 auto;
}
.label-content {
  display: flex;
  flex-wrap: wrap;
  margin-left: -16rpx;
  margin-bottom: -12rpx;
}
.label-content_style {
  line-height: 56rpx;
  padding: 0 20rpx;
  color: #f66;
  background-color: #fcf0f0;
  width: auto;
  border-radius: 40rpx;
  margin-left: 16rpx;
  margin-bottom: 12rpx;
}
