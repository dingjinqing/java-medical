        data-is_prd='{{item.is_prd}}' data-is_first_touch="{{m.is_first_touch}}" data-link="pages/bargainitem/bargainitem?bargain_id={{item.bargin_id}}" data-zhujian="{{index}}" data-idx='{{m.idx}}' wx:if="{{item.goods_type == 3}}">
