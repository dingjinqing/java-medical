.dis_recommend {
  width: 100%;
  background-color: #fff;
  padding: 0 0 20rpx 20rpx;
  margin-bottom: 26rpx;
  box-sizing: border-box;
}

.dis_recommend_title {
  border-bottom: 1px solid #eee;
  box-sizing: border-box;
  padding-right: 20rpx;
}

.dis_recommend_title > text:first-child {
  float: left;
  font-size: 30rpx;
  color: #333;
  line-height: 90rpx;
}

.dis_recommend_title > text:last-child {
  float: right;
  color: #f66;
  line-height: 90rpx;
}

.dis_recommend_title > text:last-child::after {
  content: ' ';
  display: inline-block;
  width: 18rpx;
  height: 18rpx;
  border-top: 2rpx solid #bbb;
  border-right: 2rpx solid #bbb;
  transform: rotate(45deg);
  box-sizing: border-box;
  margin-left: 5rpx;
}

