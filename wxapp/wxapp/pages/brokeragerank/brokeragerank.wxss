/* pages/brokeragerank/brokeragerank.wxss */
.clearfix::after{
    content: "";
    display: block;
    clear: both
}
page{
  height: 100%;
  background: #f5f5f5;
}
.rank_container{
  width: 750rpx;
  overflow-x: hidden;
  background: #f5f5f5;
}
.rank_tab{
  height: 100rpx;
  display: flex;
}
.rank_tab>view{
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #666;
}
.rank_tab>view view{
  font-size: 26rpx;
}
.rank_tab .actived{
  border-bottom: 2rpx solid #f66;
  color: #f66;
  padding-bottom: 5rpx;
}
.rank_top{
  width: 100%;
  height:165rpx;
  position: relative;
  padding-top: 20rpx;
}
.own_touxaing{
    width: 90rpx;
    height: 90rpx;
    border-radius: 300rpx;
    position: absolute;
    z-index: 99999;
    left: 25rpx;
    top: 30rpx;
}
.own_info{
  float: left;
  height: 90rpx;
  color: #fff;
  width: 450rpx;
  position: absolute;
  z-index: 99999;
  left: 130rpx;
  top: 30rpx;
}
.own_name{
  position: absolute;
  top: 0;
  font-size: 30rpx;
  width: 260rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.own_brokerage{
  font-size: 24rpx;
}
.rank_dai_name{
  position: absolute;
  bottom: 0;
  background: #e57777;
  color: #fff;
  font-size:22rpx;
  padding:5rpx 10rpx;
  border-radius:30rpx;
}
.own_ranking{
    color: #fff;
    font-size: 35rpx;
    float: right;
    height: 90rpx;
    line-height: 90rpx;
    margin-right: 30rpx;
    position: absolute;
    z-index: 99999;
    right: 0;
    top: 30rpx;
}
.top_bg{
  position: absolute;
  top: -1rpx;
  width: 100%;
  height: 185rpx;
  z-index: 999;
  background: linear-gradient(top,#f66,#ff9292);
  background: -webkit-linear-gradient(top,#f66,#ff9292);
  background: -moz-linear-gradient(top,#f66,#ff9292);
}
