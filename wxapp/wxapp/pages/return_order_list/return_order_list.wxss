/* pages/return/returnorder.wxss */
page{
   background: #f5f5f5;
}
.main-container{
  background: #f5f5f5;
  width: 750rpx;
}
.main {
  width: 750rpx;
}
.clearfix::after{
  contain: "";
  display: block;
  clear: both;
}
view,text{
  box-sizing: border-box;
}
.main_header {
  height: 120rpx;
  font-size: 26rpx;
  color: #333;
  padding: 0 20rpx;
  margin: 20rpx 30rpx;
  background: #fff;
  border-radius: 10rpx;
}
.main_qingqiu {
  float: right;
  height: 120rpx;
  width: 180rpx;
  display: flex;
  align-items: center;
}
.main_qingqiu button{
  display:block;
  width: 180rpx;
  height:60rpx;
  text-align:center;
  line-height:60rpx;
  font-size:26rpx;
  color:white;
  background:#f66;
  border:none;
  text-decoration:none;
  border-radius:8rpx;
  padding: 0
}
.main_qingqiu button:hover{
  border: none
}
.main_center {
  font-size: 26rpx;
  color: #333;
  padding: 0rpx 20rpx;
  padding-bottom: 30rpx;
}
.main_cen_left{
  margin-bottom: 30rpx;
}
.main_cen_left:last-of-type{
  margin-bottom: 0;
}
.main_cen_left text{
  color: #999;
}
.main_bottom {
  padding: 30rpx 20rpx 20rpx;
}
.active {
  width: 120rpx;
  height: 120rpx;
  border: 1px solid #eee;
  float: left;
}
.main_con_goods {
  font-size: 12px;
  color: #888;
}
.btn{
  display:block;
  height:72rpx;
  text-align:center;
  line-height:72rpx;
  font-size:13px;
  color:white;
  background:#f66;
  border: 1px solid #f66;
  text-decoration:none;
  border-radius:10rpx;
}
.oeder-sn{
  float: left;
  height: 120rpx;
  width:420rpx;
  font-size: 26rpx;
  color: #333;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.oeder-sn>view:first-of-type{
  margin-bottom: 10rpx;
}
.order-goodsno-box{
  font-size:13px;
}
.goods_img{
  max-width: 100%;
  display: block;
}
.main_bot_right{
  height: 120rpx;
  float: left;
  position: relative;
  padding-left: 10rpx;
  font-size: 12px;
  width: 80%;
  color: #888;
}
.goods_name {
  font-size:13px; 
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  color: #333;
}
.goods_bot{
  width: 100%;
  position: absolute;
  bottom: 0;
}
.goods_num{
  float: right;
}
.goods_price{
  float: left;
  color: #f66;
}
.main_cen_right{
  padding: 10rpx 20rpx;
}
.bot_btn{
  border: 1px solid #f66;
  color: #f66;
  padding: 0 20rpx;
  float: right;
  line-height:55rpx;
  font-size:14px;
  background: #fff;
  border-radius: 0;
}
.bot_btn::after{
  border: none;
  border-radius: 0
}
.order-goodsno-box {
  padding-top: 300rpx;
}
.order-no{
  padding: 10rpx 0px;
  font-weight: normal;
  display: flex;
  justify-content: center;
  align-items: center;
  width:100%;
}
.order-no-img {
  width: 200rpx;
  margin: 0 auto;
  margin-bottom: 30rpx;
}
.no-zi {
  color: #b8b8b8;
  text-align: center;
  font-size: 26rpx;
}
.return_sns{
  width: 690rpx;
  text-align: left;
  height: 40rpx;
  line-height: 40rpx;
  margin: 0 auto;
  margin-bottom: 20rpx;
  color: #333;
  font-size: 24rpx;
}
.buyer_info{
  width: 690rpx;
  background: #fff;
  margin: 0 auto;
  border-radius: 10rpx;
  margin-bottom: 20rpx;
}
.buyer_title{
  display: flex;
  align-items: center;
  height: 80rpx;
  margin-left: 20rpx;
  padding-right: 20rpx;
  margin-bottom: 30rpx;
  border-bottom: 2rpx solid #eee;
}
.bt_left{
  flex: 0.8;
  color: #333;
  font-weight: bold;
  font-size: 26rpx;
  text-align: left;
}
.return_state{
  flex: 1.2;
  color: #f66;
  font-size: 26rpx;
  text-align: right;
}
.return_state image{
  width: 15rpx;
}
.return_descs text{
  float: left;
}
.return_descs .return_shuom{
  color: #333;
  width: 515rpx;
}
.return_shuom image{
  width: 150rpx;
  height: 150rpx;
  margin-right: 20rpx;
  border: 2rpx solid #eee;
}
.return_shuom image:last-of-type{
  margin-right: 0
}
.btn_cancels{
  margin-left: 20rpx;
  width: 670rpx;
  border-top: 2rpx solid #eee;
  height: 90rpx;
  display:flex;
  align-items:center;
  justify-content:flex-end;
}
.btn_cancels .canek_but{
  width: 160rpx;
  height: 60rpx;
  border: 2rpx solid #eee;
  border-radius: 4rpx;
  color: #333;
  text-align:center;
  line-height:60rpx;
  font-size:26rpx;
  margin-right:20rpx
}




























