<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>

	<rule>
		<from>(.*)!small$</from>
		<to>$1</to>
	</rule>
<rule>
		<from>(.*)!middle$</from>
		<to>$1</to>
	</rule>
	<rule>
		<from>(.*)!big$</from>
		<to>$1</to>
	</rule>
	<rule>
		<from>(.*)!huge$</from>
		<to>$1</to>
	</rule>

</urlrewrite>