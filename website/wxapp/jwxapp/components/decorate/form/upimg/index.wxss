.upimg-container{
  margin-top: 20rpx;
  background: #fff;
  display: flex;
  align-items: center;
  font-size: 30rpx;
  flex-direction: column
}
.clearfix::after{
  content: "";
  display: block;
  clear: both
}
.upimg-container .upimg_title{
  padding-left: 25rpx;
  width: 726rpx;
  background: #f5f5f5;
  padding-bottom: 20rpx;
  font-size: 30rpx;
}
.upimg-container .upimg_title text{
  font-size: 24rpx;
  color: #ccc;
}
.upimg-container .img_box{
  width: 700rpx;
    padding: 20rpx 25rpx;
    padding-bottom: 0;
}
.upimg-container .img_box>view{
  float: left;
    margin: 0 20rpx 20rpx 0;
}
.upimg-container .img_box image{
    width: 200rpx;
    height: 200rpx;
     border: 2rpx solid #eee;
     
}
.upimg-container .img_box .shangchuan{
  display: inline-block;
  position: relative;
  width: 200rpx;
  height: 200rpx;
  overflow: hidden
}
.upimg-container .img_box .shangchuan .up_close{
  position: absolute;
  right: 0rpx;
  width: 35rpx;
  height: 35rpx;
  border: none;
  z-index: 999;
}