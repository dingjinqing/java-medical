.label-style13>text{
 padding-left:2rpx !important;
  padding-right:2rpx !important;
}
.label-style2>text{
  padding-bottom: 0rpx !important;
  padding-left:14rpx;
  padding-right:14rpx;
  color:white;
  font-size:22rpx;
  line-height:27rpx;
  text-align: center;
}

.label-style3,.label-style33{
  width: 40rpx !important;
  height: auto !important;
  border: 1px solid #ff6666;
  background:transparent;
  border-radius: 6rpx;
  top: 4rpx !important;
  left: 4rpx !important;
   z-index: 3;
}
.label-style3>text,.label-style33>text{
  color: #ff6666;
  font-size: 22rpx;
    padding-left:6rpx;
  padding-right:6rpx;
    padding-top: 10rpx;
  padding-bottom: 10rpx;
}

.label-style4,.label-style43{
  height: 50rpx !important;
  width: auto !important;
  background:linear-gradient(left,#f66,#ff8585);
  background:-webkit-linear-gradient(left,#f66,#ff8585);
  background:-moz-linear-gradient(left,#f66,#ff8585);
  top: 0rpx ;
  left: 0rpx;
  border-bottom-right-radius: 30rpx;
  line-height: 44rpx;
  float: left;
   z-index: 3;
}
.label-style4>text,.label-style43>text{
  color: white;
  font-size: 22rpx;
  padding-left: 16rpx;
  padding-right: 16rpx;
}
