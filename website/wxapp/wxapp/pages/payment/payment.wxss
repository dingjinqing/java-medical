}

.see-order{
  background: #ff6666;
  color: white;
  margin-bottom: 30rpx;
}

.clearfix{
  overflow: hidden;
}
.clearfix::after{
  contain: "";
  display: block;
  clear: both
}
.common-footer .footer-text {
  margin-top: 0;
  margin-bottom: 0;
  background: #f5f5f5;
}
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
