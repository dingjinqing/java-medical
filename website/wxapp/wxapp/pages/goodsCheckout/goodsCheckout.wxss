}

.score_pay, .yue_pay, .order_must {
  height: 90rpx;
  line-height: 90rpx;
}

.pay_two {
  float: left;
  position: relative;
  top: 20rpx;
}

.score_pay input, .yue_pay input, .order_must input {
  height: 50rpx;
  min-height: 50rpx;
}

.payment {
  padding-top: 20rpx;
}

.score_pay::after {
  content: "";
  display: block;
  clear: both;
}

.score_pay, .yue_pay {
  position: relative;
}

.score_pay view, .yue_pay view {
  display: inline-block;
}

.score_pay view:last-of-type, .yue_pay view:last-of-type {
