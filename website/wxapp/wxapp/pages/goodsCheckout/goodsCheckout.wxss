  text-overflow: ellipsis;
  white-space: nowrap;
  height: 32rpx;
}

.goods_text {
  position: absolute;
  bottom: -10rpx;
  width: 100%;
}

.goods_text::after {
  content: "";
  display: block;
  clear: both;
}

.goods_text .goods_price {
  float: left;
  color: #f66;
}

.goods_text .goods_num {
  float: right;
  color: #888;
}

.payway {
  padding: 10px 30rpx;
  background: #fff;
  margin-bottom: 20rpx;
  overflow-x: hidden;
}

.payway .pay_title {
