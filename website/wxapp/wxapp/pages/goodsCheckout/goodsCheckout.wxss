  min-height: 50rpx;
}

.checkout_detail {
  padding: 10px 30rpx;
  background: #fff;
  font-size: 14px;
  color: #353535;
}

.checkout_detail>view {
  line-height: 55rpx;
}

.checkout_detail>view::after {
  content: "";
  display: block;
  clear: both;
}

.checkout_detail>view view:first-of-type {
  float: left;
  color: #353535;
}

.checkout_detail>view view:last-of-type {
  float: right;
  color: #f66;
  text-align: right;
  height: 55rpx;
}

.check_bottom {
  background: #fff;
  font-size: 14px;
  color: #353535;
  position: fixed;
  width: 100%;
  bottom: 0;
  z-index: 999;
  border-top: 1px solid #eee;
}

.check_bottom::after {
  content: "";
  display: block;
  clear: both;
}

.check_bottom view:first-of-type {
  float: left;
  width: 67%;
  height: 100rpx;
  line-height: 100rpx;
  color: #f66;
  text-align: right;
  padding-right: 2%;
}

.check_bottom form button {
  float: right;
  width: 30%;
  background: #f66;
  color: #fff;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  border: none;
  border-radius: 0;
  outline: 0;
  font-size: 30rpx;
}

.check_bottom form button::after {
  border-radius: 0;
  border: none;
}

