  position: absolute;
  right: 0;
  width: 42rpx;
  text-align: center;
}

.pay_focus {
  position: absolute;
  right: 50rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  text-align: right;
}

.score_pay image, .yue_pay image {
  width: 32rpx;
  height: 10rpx;
  /* margin-left: 10rpx; */
  position: relative;
  top: 6rpx;
}

.yue_pay::after {
  content: "";
  display: block;
  clear: both;
}

.yue_pay {
  position: relative;
}

.wechat {
  padding: 10px 30rpx;
  background: #fff;
  margin-bottom: 20rpx;
  font-size: 14px;
  color: #353535;
}

