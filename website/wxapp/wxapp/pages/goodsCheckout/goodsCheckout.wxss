  font-size: 26rpx;
  margin-bottom: 20rpx;
  width: 160rpx;
}

.hr {
  width: 106%;
  height: 1px;
  background: #eee;
}

.payment radio-group {
  width: 100%;
}

.payment::after {
  content: "";
  display: block;
  clear: both;
}

.payment radio-group view {
  float: left;
  margin-right: 50rpx;
  font-size: 26rpx;
  color: #353535;
  text-align: center;
}

.pay_type {
  padding: 0px 30rpx 0;
  background: #fff;
  margin-bottom: 20rpx;
  font-size: 14px;
  color: #353535;
}

.choose_coupon::after {
  content: "";
  display: block;
  clear: both;
}

.choose_coupon view:first-of-type {
  float: left;
}

.choose_coupon view:nth-of-type(2) {
  float: left;
}

.choose_coupon view:last-of-type {
  float: right;
}

.choose_coupon view:last-of-type image {
