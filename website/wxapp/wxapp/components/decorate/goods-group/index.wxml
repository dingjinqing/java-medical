<view class="goods-group-container" id="{{m.idx}}">
  <!-- 顶部分组 -->
  <block wx:if="{{m.position_style == '0' }}">
    <view class='content_scroll_view'>
      <block wx:if="{{m.menu_style == 0}}">
        <scroll-view class='goods_group_scroll' scroll-x='true'>
          <view class='goods_group_nav'>
            <block wx:for="{{m.group_names}}" wx:key="key" wx:for-item="item" wx:for-index='index'>
              <view class='group_nav_single {{m.navlen > 4 ? "group_double_flex" : "group_single_flex"}}'>
                <view class='group_nav_single_content' bindtap='bindMenuClick' data-index="{{index}}" data-idx="{{m.idx}}">
                  <view class='group_nav_title {{index == m.group_nav_index ? "color_red":" "}}' style='color:{{index == m.group_nav_index ?comColor:""}}'>{{item}}</view>
                </view>
                <view class='nav_bottom_select' style='border-color:{{comColor}}' wx:if="{{index == m.group_nav_index }}"></view>
              </view>
            </block>
          </view>
        </scroll-view>
      </block>
      <block wx:if="{{m.menu_style == 1}}">
        <scroll-view class='goods_group_scroll {{m.fixed == true ? "nav_fixed" : ""}}' scroll-x='true' style='top:{{height * 2 + 16}}rpx'>
          <view class='goods_group_nav'>
            <block wx:for="{{m.group_names}}" wx:key="key" wx:for-item="item" wx:for-index='index'>
              <view class='group_nav_single {{m.navlen > 4 ? "group_double_flex" : "group_single_flex"}}'>
                <view class='group_nav_single_content' bindtap='bindMenuClick' data-index="{{index}}" data-idx="{{m.idx}}">
                  <view class='group_nav_title {{index == m.group_nav_index ? "color_red":" "}}' style='color:{{index == m.group_nav_index ?comColor:""}}'>{{item}}</view>
                </view>
                <view class='nav_bottom_select' wx:if="{{index == m.group_nav_index }}" style='border-color:{{comColor}}'></view>
              </view>
            </block>
          </view>
        </scroll-view>
      </block>
    </view>
    <view class='goods_group_module clearfix' style="background:{{m.goods_module_bg == 0 ? '': m.goods_bg_color}}">
      <!-- 大图 -->
      <view class='group_big_module' wx:if="{{m.shop_style == 1}}">
        <block data-idx="{{m.idx}}" wx:for="{{m.first_group_goods}}" wx:key='key' wx:for-item="item" wx:for-index='index'>
          <!-- 普通商品 -->
          <view class='group_container  {{m.if_radius == 1 ? "circle_radius": " "}} {{m.module_style == 2 ? "have_shadow":" "}} {{m.module_style == 3 ? "have_border" : ""}}' catchtap="{{item.goods_type == 10 ? 'bindTopre' : 'bindJumpLink'}}" data-is_first_touch="{{m.is_first_touch}}"
            data-idx='{{m.idx}}' data-presale_id='{{item.presale_id}}' data-goods_type='{{item.goods_type}}' data-url="pages/item/item?good_id={{item.goods_id}}" wx:if='{{item.goods_type !=3 && item.goods_type !=1 && item.goods_type !=5}}'>
            <view class='module_img'>
              <include src="../goodscommon/labelcommon.wxml" />
              <image class="sell_out" wx:if='{{item.goods_number == 0}}' src="{{m.imgUrl}}image/wxapp/goods_sell_out.png"></image>
              <view class='reduce_price' wx:if="{{item.goods_price_action == 2 && item.goods_type != 10}}" style='background:{{linColor}};color:#fff'>限时降价，立即查看</view>
              <view class='reduce_price' wx:if="{{item.goods_price_action == 3}}" style='background:{{linColor}};color:#fff'>新人专享，首单特惠</view>
              <view class='exclusive' wx:elif="{{item.is_card_exclusive == 1 && item.goods_type != 1 && item.goods_type != 3 && item.goods_type != 5 && item.goods_type != 10 && item.goods_price_action != 2 && item.goods_price_action != 3}}">会员专享商品，立即查看</view>
              <view class='grade' wx:if="{{item.goods_price_action == 1 && item.goods_type != 10}}">专享会员价，立即查看</view>
              <view class='group_delete' wx:if='{{item.is_delete == 1}}'>商品已删除</view>
              <image lazy-load='{{true}}' src='{{item.goods_img}}!big'></image>
            </view>
            <view class='module_text'>
              <view class='module_goods_name' style='display:{{m.show_name == 1 ? "none":"block"}}'>
                <block wx:if='{{item.goods_type == 10 || item.goods_tags != ""}}'>
                  <view class='van-ellipsis'>{{item.goods_name}}</view>
                  <view class='title_label'>
                    <view class='pin_flag' wx:if='{{item.goods_type == 10}}' style='color:{{comColor}};border-color:{{comColor}}'>预售</view>
                    <view class='pin_flag' wx:if='{{item.goods_tags != ""}}' style='color:{{comColor}};border-color:{{comColor}}'>{{item.goods_tags[0]}}</view>
                    <view class='pin_flag' wx:if='{{item.goods_tags[1] != undefined && item.goods_type != 10}}' style='color:{{comColor}};border-color:{{comColor}}'>{{item.goods_tags[1]}}</view>
                  </view>
                </block>
                <block wx:else>
                  <view class='van-two-ellipsis'>{{item.goods_name}}</view>
                </block>
              </view>
              <view class='module_goods_price'>
                <block wx:if="{{m.show_price != 1}}">
                  <view class='sale_price' wx:if='{{item.goods_type != 10}}' style='color:{{comColor}};'>￥{{item.reduce_price}}</view>
                  <view class='sale_price' wx:if='{{item.goods_type == 10}}' style='color:{{comColor}};'>￥{{item.shop_price}}</view>
                </block>
                <block wx:if='{{m.other_message == 0  && m.cart_btn != 0}}'>
                  <block wx:if="{{m.show_market == 1}}">
                    <view class='market_price' wx:if="{{item.line_price != undefined && item.line_price != '0.00'}}">￥{{item.line_price}}</view>
                  </block>
                  <block wx:if="{{m.show_market == 2}}">
                    <view class='market_price line-through-none'>{{item.goods_sale_num > 999 ? '999+' : item.goods_sale_num}}人付款</view>
                  </block>
                  <block wx:if="{{m.show_market == 3}}">
                    <view class='market_price line-through-none'>{{item.comment_num > 999 ? '999+' : item.comment_num}}人评价</view>
                  </block>
                </block>
                <block wx:if="{{m.cart_btn == 0 && item.goods_type != 10 }}">
                  <text class='cart_btn iconfont icontianjia' style='font-size:46rpx;color:{{comColor}}' wx:if="{{m.cart_btn_choose == '0'}}" catchtap='bindAddCart' data-goods_id='{{item.goods_id}}'></text>
                  <text class='cart_btn iconfont icongouwuche1' style='font-size:52rpx;color:{{comColor}}' wx:if="{{m.cart_btn_choose == '1'}}" catchtap='bindAddCart' data-goods_id='{{item.goods_id}}'></text>
                  <text class='cart_btn right_buy_btn' style='background:{{comColor}}' wx:if="{{m.cart_btn_choose == '2'}}" catchtap='bindAddCart' data-goods_id='{{item.goods_id}}'>马上抢</text>
                  <text class='cart_btn cart_buy_btn' style='color:{{comColor}};border:1px solid {{comColor}}' wx:if="{{m.cart_btn_choose == '3'}}" catchtap='bindAddCart' data-goods_id='{{item.goods_id}}'>购买</text>
                </block>
              </view>
            </view>
          </view>
          <!-- 拼团 -->
          <view class='group_container  {{m.if_radius == 1 ? "circle_radius": " "}} {{m.module_style == 2 ? "have_shadow":" "}} {{m.module_style == 3 ? "have_border" : ""}}' catchtap="bindJumpLink" data-is_first_touch="{{m.is_first_touch}}" data-idx='{{m.idx}}'
            data-url="pages/groupbuyitem/groupbuyitem?pin_group_id={{m.pin_group_goods[item.goods_id].pin_activity_id}}" wx:if='{{m.pin_group_goods[item.goods_id] && item.goods_type ==1}}'>
            <view class='module_img'>
              <include src="../goodscommon/labelcommon.wxml" />
              <image class="sell_out" wx:if='{{item.goods_number == 0}}' src="{{m.imgUrl}}image/wxapp/goods_sell_out.png"></image>
              <view class='group_delete' wx:if='{{item.is_delete == 1}}'>商品已删除</view>
              <image lazy-load='{{true}}' src='{{item.goods_img}}!big'></image>
              <view class='save_money' wx:if='{{m.pin_group_goods[item.goods_id]}}' style='background:-webkit-linear-gradient(left,{{linColor}},{{comColor1}})'>开团立省{{m.pin_group_goods[item.goods_id].less_money}}元</view>
            </view>
            <view class='module_text'>
              <view class='module_goods_name' style='display:{{m.show_name == 1 ? "none":"block"}}'>
                <view class='van-ellipsis'>{{item.goods_name}}</view>
                <view class='title_label'>
                  <view class='pin_flag' style='color:{{comColor}};border-color:{{comColor}}'>拼团</view>
                  <view class='pin_flag' wx:if='{{item.goods_tags != ""}}' style='color:{{comColor}};border-color:{{comColor}}'>{{item.goods_tags[0]}}</view>
                </view>

              </view>
              <view class='module_goods_price' style='display:{{m.show_price == 1?"none":"block"}}'>
                <view class='sale_price' style='color:{{comColor}};'>￥{{m.pin_group_goods[item.goods_id].pin_group_price}}</view>
                <block wx:if='{{m.other_message == 0  && m.cart_btn != 0}}'>
                  <block wx:if="{{m.show_market == 1}}">
                    <view class='market_price' wx:if="{{item.line_price != undefined && item.line_price != '0.00'}}">￥{{item.line_price}}</view>
                  </block>
                  <block wx:if="{{m.show_market == 2}}">
                    <view class='market_price line-through-none'>{{item.goods_sale_num > 999 ? '999+' : item.goods_sale_num}}人付款</view>
                  </block>
                  <block wx:if="{{m.show_market == 3}}">
