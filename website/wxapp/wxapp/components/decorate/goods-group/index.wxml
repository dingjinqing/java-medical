                  <view class='van-two-ellipsis'>{{item.goods_name}}</view>
                </view>
                <view class='title_label'>
                  <view class='pin_flag' style='color:{{comColor}};border-color:{{comColor}}'>砍价</view>
                  <view class='pin_flag' wx:if='{{item.goods_tags != ""}}' style='color:{{comColor}};border-color:{{comColor}}'>{{item.goods_tags[0]}}</view>
                </view>

              </view>
              <view class='module_goods_name' wx:if='{{m.show_name == 1}}'></view>
              <view class='module_goods_price' style='display:{{m.show_price == 1?"none":"block"}}'>

                <view class='sale_price' style='color:{{comColor}}'>￥{{item.bargain_price}}</view>
                <block wx:if='{{m.other_message == 0  && m.cart_btn != 0}}'>
                  <block wx:if="{{m.show_market == 1 && item.price_len < 14 }}">
                    <view class='market_price' wx:if="{{item.line_price != undefined && item.line_price != '0.00'}}">￥{{item.line_price}}</view>
                  </block>
                  <block wx:if="{{m.show_market == 2 && item.real_len < 8 }}">
                    <view class='market_price line-through-none'>{{item.goods_sale_num > 999 ? '999+' : item.goods_sale_num}}人付款</view>
                  </block>
                  <block wx:if="{{m.show_market == 3 && item.real_len < 8}}">
                    <view class='market_price line-through-none'>{{item.comment_num > 999 ? '999+' : item.comment_num}}人评价</view>
                  </block>
                </block>
              </view>
            </view>
          </view>
          <!-- 秒杀-->
          <view class='group_container  {{m.if_radius == 1 ? "circle_radius": " "}} {{m.module_style == 2 ? "have_shadow":" "}} {{m.module_style == 3 ? "have_border" : ""}}' catchtap="bindToSecKill" data-is_first_touch="{{m.is_first_touch}}" data-sk_id='{{item.sk_id}}'
            data-idx='{{m.idx}}' data-link="pages/seckillitem/seckillitem?sk_id={{item.sk_id}}" wx:if='{{ item.goods_type ==5 }}'>
            <view class='module_img'>
              <include src="../goodscommon/labelcommon.wxml" />
              <image class="sell_out" wx:if='{{item.goods_number == 0}}' src="{{m.imgUrl}}image/wxapp/goods_sell_out.png"></image>
              <view class='group_delete' wx:if='{{item.is_delete == 1}}'>商品已删除</view>
              <image lazy-load='{{true}}' src='{{item.goods_img}}!big'></image>
            </view>
            <view class='module_text'>
              <view class='module_goods_name' wx:if='{{m.show_name != 1}}'>
                <view class='top_title'>
                  <view class='van-two-ellipsis'>{{item.goods_name}}</view>
                </view>
                <view class='title_label'>
                  <view class='pin_flag' style='color:{{comColor}};border-color:{{comColor}}'>拼团</view>
                  <view class='pin_flag' wx:if='{{item.goods_tags != ""}}' style='color:{{comColor}};border-color:{{comColor}}'>{{item.goods_tags[0]}}</view>
                </view>

              </view>
              <view class='module_goods_name' wx:if='{{m.show_name == 1}}'></view>
              <view class='module_goods_price' style='display:{{m.show_price == 1?"none":"block"}}'>

                <view class='sale_price' style='color:{{comColor}}'>￥{{item.seckill_price}}</view>
                <block wx:if='{{m.other_message == 0  && m.cart_btn != 0}}'>
                  <block wx:if="{{m.show_market == 1 && item.price_len < 14 }}">
                    <view class='market_price' wx:if="{{item.line_price != undefined && item.line_price != '0.00'}}">￥{{item.line_price}}</view>
                  </block>
                  <block wx:if="{{m.show_market == 2 && item.real_len < 8 }}">
                    <view class='market_price line-through-none'>{{item.goods_sale_num > 999 ? '999+' : item.goods_sale_num}}人付款</view>
                  </block>
                  <block wx:if="{{m.show_market == 3 && item.real_len < 8}}">
                    <view class='market_price line-through-none'>{{item.comment_num > 999 ? '999+' : item.comment_num}}人评价</view>
                  </block>
                </block>
              </view>
            </view>
          </view>
        </block>
        <view class='click_see_more' catchtap='bindMenuClick' data-click='{{1}}' data-index="{{index}}" data-idx="{{m.idx}}" wx:if="{{m.more_flag == 1}}">
          <view class='click_see'>
            <text>查看更多</text>
            <image lazy-load='{{true}}' src='{{m.imgUrl}}image/wxapp/see_more_down.png'></image>
          </view>
        </view>
      </view>
    </view>
  </block>
</view>
